class Solution {
    public boolean isBalanced(String num) {
        int evensum=0;
        int oddsum=0;
        for(int i=0;i<num.length();i++){
            int temp=num.charAt(i)-'0';
            if(i%2==0)
               evensum+=temp;

            else{
               oddsum+=temp;
            }
           
            
        }
        if(evensum==oddsum)
            return true;
        else
           return false;
    }
}
