class Solution {
    public boolean isPalindrome(int x) {
        int reversed=0;
        int original =x;
        int digit;
        if(x<0)
           return false;
        while(x>0){
            digit=x%10;
            reversed=reversed*10+digit;
            x=x/10;
        }
        if(original==reversed)
            return true;
        else
            return false;
    }
}
